{
    "build": {
      "arduino":{
        "ldscript": "esp32c3_out.ld"
      },
      "core": "esp32",
      "extra_flags": "-DARDUINO_USB_CDC_ON_BOOT=0 -DESP32_2M -DESP32C3",
      "f_cpu": "120000000L",
      "f_flash": "60000000L",
      "flash_mode": "dio",
      "mcu": "esp32c2",
      "variant": "esp32c2",
      "partitions": "partitions/esp32_2M_partition_app1792k_no_ota_spiffs192k.csv"
    },
    "connectivity": [
      "wifi",
      "bluetooth"
    ],
    "debug": {
      "openocd_target": "esp32c2.cfg"
    },
    "frameworks": [
      "arduino",
      "espidf"
    ],
    "name": "Espressif Generic ESP32-C3 2M Flash, Tasmota 1792k Code No OTA, 192k FS",
    "upload": {
      "flash_size": "2MB",
      "maximum_ram_size": 327680,
      "maximum_size": 2097152,
      "require_upload_port": true,
      "speed": 115200
    },
    "url": "https://docs.espressif.com/projects/esp-idf/en/latest/esp32c2/get-started/index.html#hardware",
    "vendor": "Espressif"
  }
